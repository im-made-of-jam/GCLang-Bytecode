build:
g++ ./main.cpp -o ./main.exe -I . -std=gnu++20 -Wall
   "g++"           - compiler
   "./main.cpp"    - this file
   "-o ./main.exe" - output file
   "-I ."          - include path includes base directory so relative includes are not needed
   "-std=gnu++20"  - modern standard so std::filesystem works
   "-Wall"         - report all warnings

git commit:
git commit -m "*message*"

git push:
git push --set-upstream origin main -f
   yes the -f is probably bad practice but it makes it work so ¯\_(ᓀᓀ)_/¯

git pull:
git pull https://github.com/im-made-of-jam/GCLang-Bytecode.git

git add:
git add ./*